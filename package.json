    "build": "npm run compile && npm run copyfiles",
    "lint": "eslint . --ext=.ts"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "keywords": [
    "bull",
    "queue",
    "adonis",
    "adonisjs",
    "adonis-js",
    "adonis-bull",
    "adonis-queue"
  ],
  "bugs": {
    "url": "https://github.com/rocketseat/adonis-bull/issues"
  },
  "homepage": "https://github.com/rocketseat/adonis-bull#readme",
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "devDependencies": {
    "@adonisjs/ace": "^6.9.3",
    "@adonisjs/application": "^2.0.0",
    "@adonisjs/core": "^5.0.0-preview-rc-1.12",
    "@adonisjs/fold": "^6.4.0",
    "@adonisjs/logger": "^2.1.0",
    "@adonisjs/mrm-preset": "^2.4.0",
    "@types/node": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "copyfiles": "^2.3.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "japa": "^3.1.1",
    "nyc": "^15.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocketseat/adonis-bull.git"
  },
  "peerDependencies": {
    "@adonisjs/core": ">=5.0.0-preview"
  },
  "dependencies": {
    "bull-board": "^1.0.0-alpha.10",
    "bullmq": "^1.9.0"
  },
  "files": [
    "build/adonis-typings",
    "build/commands",
    "build/providers",
    "build/src",
    "build/templates"
  ],
  "types": "build/adonis-typings/index.d.ts",
  "adonisjs": {
    "types": "@rocketseat/adonis-bull",
    "providers": [
      "@rocketseat/adonis-bull"
    ],
    "commands": [
      "@rocketseat/adonis-bull/build/commands"
    ]
  }
}
