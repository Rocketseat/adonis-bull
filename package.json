{
  "name": "@rocketseat/adonis-bull",
  "version": "0.2.2",
  "main": "build/providers/BullProvider.js",
  "license": "MIT",
  "scripts": {
    "test:local": "node japaFile.js",
    "test": "nyc npm run test:local",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "copyfiles": "copyfiles \"templates/**/*.txt\" build",
    "compile": "tsc",
    "build": "npm run compile && npm run copyfiles",
    "lint": "eslint --ignore-path .gitignore",
    "format": "prettier \"**/*.ts\" --write --ignore-path .gitignore"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "directories": {
    "test": "test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "commitlint": {
    "extends": [
      "rocketseat"
    ]
  },
  "keywords": [
    "bull",
    "queue",
    "adonis",
    "adonisjs",
    "adonis-js",
    "adonis-bull",
    "adonis-queue"
  ],
  "bugs": {
    "url": "https://github.com/rocketseat/adonis-bull/issues"
  },
  "homepage": "https://github.com/rocketseat/adonis-bull#readme",
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "devDependencies": {
    "@adonisjs/ace": "^6.9.3",
    "@adonisjs/application": "^2.0.0",
    "@adonisjs/core": "^5.0.4-preview-rc-2.1",
    "@adonisjs/fold": "^6.4.0",
    "@adonisjs/logger": "^2.1.0",
    "@adonisjs/mrm-preset": "^2.4.0",
    "@adonisjs/sink": "^1.0.17",
    "@poppinss/dev-utils": "^1.0.11",
    "@types/node": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "copyfiles": "^2.3.0",
    "japa": "^3.1.1",
    "nyc": "^15.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7",
    "@commitlint/cli": "^11.0.0",
    "commitlint-config-rocketseat": "^0.0.1",
    "coveralls": "^3.0.7",
    "delay": "^4.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocketseat/adonis-bull.git"
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.0.4-preview-rc"
  },
  "dependencies": {
    "bull-board": "^1.2.0",
    "bullmq": "^1.9.0"
  },
  "files": [
    "build/adonis-typings",
    "build/commands",
    "build/providers",
    "build/src",
    "build/templates",
    "build/instructions.js",
    "build/instructions.d.ts"
  ],
  "typings": "build/adonis-typings/index.d.ts",
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "types": "@rocketseat/adonis-bull",
    "providers": [
      "@rocketseat/adonis-bull"
    ],
    "commands": [
      "@rocketseat/adonis-bull/build/commands"
    ]
  }
}
